{
  "name": "Twitter removal",
  "version": "1.0",
  "description": "Twitter removal tool for Chrome",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}